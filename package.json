{
  "name": "gulp-normalize-pkg",
  "description": "Gulp plugin for normalize-pkg.",
  "version": "0.1.1",
  "homepage": "https://github.com/jonschlinkert/gulp-normalize-pkg",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-normalize-pkg",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-normalize-pkg/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "normalize-pkg": "^0.3.13",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3",
    "vinyl": "^1.1.1"
  },
  "keywords": [
    "gulp",
    "json",
    "normalize",
    "package",
    "package-json",
    "pkg"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "normalize-pkg",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
